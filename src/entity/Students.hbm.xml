<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entity.Students" table="STUDENTS">
		<id name="sid" type="java.lang.String" length="8">
			<generator class="assigned"/>
		</id>
		<property name="sname" type="java.lang.String"/>
		<property name="gender" type="java.lang.String"/>
		<property name="birthday" type="date"/>
		<property name="address" type="java.lang.String"/>
		<property name="specialty" type="java.lang.String"/>
		<property name="sage" type="java.lang.String"/>
		<property name="education" type="java.lang.String"/>
		<property name="type" type="java.lang.String"/>
		<property name="graduation" type="java.lang.String"/>
		<property name="certificate" type="java.lang.String"/>
		<property name="height" type="java.lang.String"/>
		<property name="school" type="java.lang.String"/>
		<property name="marry" type="java.lang.String"/>
		<property name="driver" type="java.lang.String"/>
		<property name="phone" type="java.lang.String"/>
		<property name="qq" type="java.lang.String"/>
		<property name="vchat" type="java.lang.String"/>
	</class>
</hibernate-mapping>
